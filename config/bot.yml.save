name: darebot
cli: false
chat_application: slack

# OAuth & Permissions > Bot User OAuth Access Token
slack_token: ${SLACK_TOKEN}
       
# App credentials > Verification Token
slack_verification_token: ${SLACK_VERIFICATION_TOKEN}

# OAuth & Permissions > OAuth Access Token
slack_workspace_token: ${SLACK_WORKSPACE_TOKEN}

slack_events_callback_path: /slack_events/v1/events
slack_interactions_callback_path: /slack_events/v1/interactions
